Algoritmo "TROCA DE VARIAVEIS"


Var
   n1, n2, temp: inteiro
   i: inteiro
Inicio

   escreval("escreva o número 1")
   leia(n1)
   escreval("escreva o número 2")
   leia(n2)
   temp <- n1
   n1<- n2
   n2<- temp

   escreval("após a troca o numero 1 ficou", n1)
   escreval("após a troca o numero 2 ficou", n2)


fimalgoritmo
